{
  "name": "basic-template",
  "version": "0.0.1",
  "description": "A basic TypeScript project template using Parcel bundler and ES modules.",
  "type": "module",
  "scripts": {
    "preinstall": "npx scan-compromised",
    "dev": "parcel src/index.pug",
    "build": "parcel build src/index.pug",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "clean": "rm -rf .parcel-cache dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.16.0",
    "@parcel/transformer-pug": "^2.16.0",
    "@types/pug": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-tsdoc": "^0.4.0",
    "parcel": "^2.12.0",
    "prettier": "^3.6.2",
    "pug": "^3.0.3",
    "typescript": "^5.4.0"
  },
  "keywords": [
    "typescript",
    "parcel",
    "esmodules",
    "template"
  ],
  "author": "Jonathan Blades <jonoblades@gmail.com>",
  "license": "MIT"
}
